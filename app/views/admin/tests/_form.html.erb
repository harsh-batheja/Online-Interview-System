<%= form_for [:admin,@test], :html => { :class => "form-horizontal test" } do |f| %>
  <% if @test.errors.any? %>
    <div id="error_expl" class="panel panel-danger">
      <div class="panel-heading">
        <h3 class="panel-title"><%= pluralize(@test.errors.count, "error") %> prohibited this test from being saved:</h3>
      </div>
      <div class="panel-body">
        <ul>
        <% @test.errors.full_messages.each do |msg| %>
          <li><%= msg %></li>
        <% end %>
        </ul>
      </div>
    </div>
  <% end %>

  <div class="form-group">
    <%= f.label :paper_id, :class => 'control-label col-lg-2' %>
    <div class="col-lg-10">
      <%= f.text_field :paper_id, :class => 'form-control' %>
    </div>
    <%=f.error_span(:paper_id) %>
  </div>
  <div class="form-group">
    <%= f.label :user_id, :class => 'control-label col-lg-2' %>
    <div class="col-lg-10">
      <%= f.text_field :user_id, :class => 'form-control' %>
    </div>
    <%=f.error_span(:user_id) %>
  </div>
  <div class="form-group">
    <%= f.label :marks, :class => 'control-label col-lg-2' %>
    <div class="col-lg-10">
      <%= f.text_field :marks, :class => 'form-control' %>
    </div>
    <%=f.error_span(:marks) %>
  </div>

  <div class="form-group">
    <div class="col-lg-offset-2 col-lg-10">
      <%= f.submit nil, :class => 'btn btn-primary' %>
      <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                admin_user_paper_tests_path(@test.user,@test.paper), :class => 'btn btn-default' %>
    </div>
  </div>

<% end %>
